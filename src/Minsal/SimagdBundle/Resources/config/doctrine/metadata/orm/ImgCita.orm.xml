<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ImgCita" table="img_cita">
    <indexes>
      <index name="IDX_F0266B5A9F2BAE3A" columns="id_modalidad_imagenologia_establecimiento_citacion"/>
      <index name="IDX_F0266B5A94328D4F" columns="id_preinscripcion"/>
      <index name="IDX_F0266B5A3B94B0C1" columns="id_parentesco"/>
      <index name="IDX_F0266B5ADD0D821B" columns="id_estado_cita"/>
      <index name="IDX_F0266B5A7DFA12F6" columns="id_establecimiento"/>
      <index name="IDX_F0266B5A890253C7" columns="id_empleado"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="fechaProgramada" type="datetime" column="fecha_programada" nullable="false"/>
    <field name="reprogramada" type="boolean" column="reprogramada" nullable="true"/>
    <field name="fechaProgramadaAnterior" type="datetime" column="fecha_programada_anterior" nullable="true"/>
    <field name="incidenciasCita" type="string" column="incidencias_cita" length="100" nullable="true"/>
    <field name="observacionesCita" type="string" column="observaciones_cita" length="100" nullable="true"/>
    <field name="razonAnulada" type="string" column="razon_anulada" length="50" nullable="true"/>
    <field name="fechaCreacionCita" type="datetime" column="fecha_creacion_cita" nullable="true"/>
    <field name="fechaConfirmacionCita" type="datetime" column="fecha_confirmacion_cita" nullable="true"/>
    <field name="fechaReprogramacionCita" type="datetime" column="fecha_reprogramacion_cita" nullable="true"/>
    <field name="necesitaAutorizacion" type="boolean" column="necesita_autorizacion" nullable="true"/>
    <field name="citaAutorizada" type="boolean" column="cita_autorizada" nullable="true"/>
    <field name="nombreResponsableAutoriza" type="string" column="nombre_responsable_autoriza" nullable="true"/>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpleado" target-entity="MntEmpleado">
      <join-columns>
        <join-column name="id_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstadoCita" target-entity="CtlEstadoCita">
      <join-columns>
        <join-column name="id_estado_cita" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idParentesco" target-entity="CtlParentesco">
      <join-columns>
        <join-column name="id_parentesco" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idPreinscripcion" target-entity="ImgPreinscripcion">
      <join-columns>
        <join-column name="id_preinscripcion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idModalidadImagenologiaEstablecimientoCitacion" target-entity="CtlModalidadImagenologiaEstablecimientoCitacion">
      <join-columns>
        <join-column name="id_modalidad_imagenologia_establecimiento_citacion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
