<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ImgNotaDiagnostico" table="img_nota_diagnostico">
    <indexes>
      <index name="IDX_9F90E301838319BF" columns="id_diagnostico"/>
      <index name="IDX_9F90E3017DFA12F6" columns="id_establecimiento"/>
      <index name="IDX_9F90E3019FA5140" columns="id_tipo_nota_diagnostico"/>
      <index name="IDX_9F90E301890253C7" columns="id_empleado"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="notaDiagnostico" type="text" column="nota_diagnostico" nullable="true"/>
    <field name="fechaNotaDiagnostico" type="datetime" column="fecha_nota_diagnostico" nullable="true"/>
    <field name="observaciones" type="string" column="observaciones" length="100" nullable="true"/>
    <many-to-one field="idEmpleado" target-entity="MntEmpleado">
      <join-columns>
        <join-column name="id_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTipoNotaDiagnostico" target-entity="CtlTipoNotaDiagnostico">
      <join-columns>
        <join-column name="id_tipo_nota_diagnostico" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idDiagnostico" target-entity="ImgDiagnostico">
      <join-columns>
        <join-column name="id_diagnostico" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
