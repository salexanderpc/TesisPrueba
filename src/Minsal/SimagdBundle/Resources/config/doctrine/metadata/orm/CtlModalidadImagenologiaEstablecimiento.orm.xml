<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CtlModalidadImagenologiaEstablecimiento" table="ctl_modalidad_imagenologia_establecimiento">
    <indexes>
      <index name="IDX_C90DC3D6C02E5491" columns="id_modalidad_imagenologia"/>
      <index name="IDX_C90DC3D67DFA12F6" columns="id_establecimiento"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_modalidad_establecimiento" columns="id_establecimiento,id_modalidad_imagenologia"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="modalidadRealizableHabilitada" type="boolean" column="modalidad_realizable_habilitada" nullable="false"/>
    <field name="fechaInicioRealizable" type="date" column="fecha_inicio_realizable" nullable="true"/>
    <field name="descripcionModalidadRealizable" type="string" column="descripcion_modalidad_realizable" length="50" nullable="true"/>
    <field name="observacionesModalidadRealizable" type="string" column="observaciones_modalidad_realizable" length="50" nullable="true"/>
    <field name="duracionClinicaEstudio" type="smallint" column="duracion_clinica_estudio" nullable="true"/>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idModalidadImagenologia" target-entity="CtlModalidadImagenologia">
      <join-columns>
        <join-column name="id_modalidad_imagenologia" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
