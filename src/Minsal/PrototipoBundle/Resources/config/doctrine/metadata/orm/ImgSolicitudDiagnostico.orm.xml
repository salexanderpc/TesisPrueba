<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ImgSolicitudDiagnostico" table="img_solicitud_diagnostico">
    <indexes>
      <index name="IDX_8502380394328D4F" columns="id_preinscripcion"/>
      <index name="IDX_85023803213355AE" columns="id_estudio_paciente"/>
      <index name="IDX_850238039A3949E3" columns="id_establecimiento_solicitado"/>
      <index name="IDX_85023803890253C7" columns="id_empleado"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="solicitudDiagnosticoRemota" type="boolean" column="solicitud_diagnostico_remota" nullable="true"/>
    <field name="fechaCreacionSolicitud" type="datetime" column="fecha_creacion_solicitud" nullable="true"/>
    <field name="justificacionSolicitud" type="string" column="justificacion_solicitud" length="100" nullable="false"/>
    <field name="fechaProximaConsulta" type="date" column="fecha_proxima_consulta" nullable="true"/>
    <field name="observacionesSolicitud" type="string" column="observaciones_solicitud" length="100" nullable="true"/>
    <many-to-one field="idEmpleado" target-entity="MntEmpleado">
      <join-columns>
        <join-column name="id_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstablecimientoSolicitado" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_establecimiento_solicitado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstudioPaciente" target-entity="ImgEstudioPaciente">
      <join-columns>
        <join-column name="id_estudio_paciente" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idPreinscripcion" target-entity="ImgPreinscripcion">
      <join-columns>
        <join-column name="id_preinscripcion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
