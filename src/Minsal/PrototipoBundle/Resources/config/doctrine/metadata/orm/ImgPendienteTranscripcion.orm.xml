<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ImgPendienteTranscripcion" table="img_pendiente_transcripcion">
    <indexes>
      <index name="IDX_A5021FE218971421" columns="id_lectura"/>
      <index name="IDX_A5021FE27DFA12F6" columns="id_establecimiento"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="fechaIngresoLista" type="datetime" column="fecha_ingreso_lista" nullable="true"/>
    <field name="fueImpugnado" type="boolean" column="fue_impugnado" nullable="true"/>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idLectura" target-entity="ImgLectura">
      <join-columns>
        <join-column name="id_lectura" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
