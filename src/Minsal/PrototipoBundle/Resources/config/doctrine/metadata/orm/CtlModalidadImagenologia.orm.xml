<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CtlModalidadImagenologia" table="ctl_modalidad_imagenologia">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombreModalidad" type="string" column="nombre_modalidad" nullable="false"/>
    <field name="codigoModalidad" type="string" column="codigo_modalidad" nullable="true"/>
    <field name="modalidadHabilitada" type="boolean" column="modalidad_habilitada" nullable="true"/>
    <field name="descripcionModalidad" type="string" column="descripcion_modalidad" length="100" nullable="true"/>
    <field name="observacionesModalidad" type="string" column="observaciones_modalidad" length="50" nullable="true"/>
  </entity>
</doctrine-mapping>
