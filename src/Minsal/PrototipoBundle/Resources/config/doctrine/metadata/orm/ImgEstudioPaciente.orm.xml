<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ImgEstudioPaciente" table="img_estudio_paciente">
    <indexes>
      <index name="IDX_20B8CA259E9497EB" columns="id_procedimiento_realizado"/>
      <index name="IDX_20B8CA257DFA12F6" columns="id_establecimiento"/>
      <index name="IDX_20B8CA25701624C4" columns="id_expediente"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="fechaEstudio" type="datetime" column="fecha_estudio" nullable="true"/>
    <field name="estudioUid" type="integer" column="estudio_uid" nullable="false"/>
    <field name="seriesUid" type="string" column="series_uid" length="100" nullable="false"/>
    <field name="servidor" type="string" column="servidor" nullable="true"/>
    <many-to-one field="idExpediente" target-entity="MntExpediente">
      <join-columns>
        <join-column name="id_expediente" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idProcedimientoRealizado" target-entity="ImgProcedimientoRealizado">
      <join-columns>
        <join-column name="id_procedimiento_realizado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
